    <script type="module" src="/js/main.js" defer></script>
    <script src="/js/auth.js" defer></script>

    <script>
        document.addEventListener('DOMContentLoaded', () => {
            // Khởi tạo Splide Carousel
            if (typeof Splide !== 'undefined') { 
                const seriesCarousel = document.getElementById('series-carousel');
                if (seriesCarousel) { 
                    new Splide(seriesCarousel, {
                        type      : 'loop',
                        perPage   : 1,
                        autoplay  : true,
                        interval  : 5000,
                        pauseOnHover: true,
                        arrows    : true,
                        pagination: false,
                    }).mount();
                }
            } else {
                console.error('Splide JS chưa được tải.');
            }

            // Logic cho thanh filter
            const filterButtons = document.querySelectorAll('.filter-bar button');
            filterButtons.forEach(button => {
                button.addEventListener('click', () => {
                    filterButtons.forEach(btn => {
                        btn.classList.remove('filter-button-active');
                        btn.classList.add('filter-button-inactive');
                    });
                    button.classList.add('filter-button-active');
                    button.classList.remove('filter-button-inactive');
                    // Thêm logic filter của bạn ở đây...
                });
            });

            // --- ===== BẮT ĐẦU JAVASCRIPT MỚI CHO BỘ LỌC NGÀY ===== ---
            
            // Hàm xử lý logic ẩn/hiện chung
            function handleDateRangeChange(selectElement, targetContainer) {
                if (selectElement.value === 'custom') {
                    targetContainer.style.display = 'grid'; // Hiển thị dạng lưới (grid)
                } else {
                    targetContainer.style.display = 'none';
                }
            }

            // Lấy các phần tử DOM cho bộ lọc MOBILE
            const postDateSelectMobile = document.getElementById('post-date-mobile');
            const customRangeMobile = document.getElementById('custom-date-range-mobile');
            
            // Lấy các phần tử DOM cho bộ lọc DESKTOP
            const postDateSelectDesktop = document.getElementById('post-date');
            const customRangeDesktop = document.getElementById('custom-date-range-desktop');

            // Gán sự kiện cho bản MOBILE
            if (postDateSelectMobile && customRangeMobile) {
                // Đặt trạng thái ban đầu khi tải trang
                handleDateRangeChange(postDateSelectMobile, customRangeMobile); 
                // Gán sự kiện 'change'
                postDateSelectMobile.addEventListener('change', () => {
                    handleDateRangeChange(postDateSelectMobile, customRangeMobile);
                });
            }

            // Gán sự kiện cho bản DESKTOP
            if (postDateSelectDesktop && customRangeDesktop) {
                // Đặt trạng thái ban đầu khi tải trang
                handleDateRangeChange(postDateSelectDesktop, customRangeDesktop);
                // Gán sự kiện 'change'
                postDateSelectDesktop.addEventListener('change', () => {
                    handleDateRangeChange(postDateSelectDesktop, customRangeDesktop);
                });
            }
            // --- ===== KẾT THÚC JAVASCRIPT MỚI ===== ---
        });
    </script>
