{
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": [
        "node_modules/sodium-native/**",
        "node_modules/sodium-native/index.js",
        "node_modules/sodium-native/prebuilds/**",
        "node_modules/sodium-native/build/**"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}